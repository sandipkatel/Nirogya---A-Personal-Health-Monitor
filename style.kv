                    MDRectangleFlatButton:
                        halign: 'center'
                        valign: 'center'
                        on_release: root.on_click('shekhar')
                        size_hint: 1, 1
                        md_bg_color: 1, 1, 1, 1
                        AnchorLayout:
                            anchor_x: "center"
                            FitImage:
                                source: "./dependencies/pics/Doctor2.png"
                                halign: "center"

                MDLabel:
                    text:"Dr.Shekhar Joshi"
                    font_size: "32sp"
                    halign: "center"
                    pos_hint: {"center_x":.5,"center_y":.2}
                MDLabel:
                    text:"Dentist"
                    font_size: "22sp"
                    halign: "center"
                    pos_hint: {"center_x":.5,"center_y":.1}
                        

<SchedulerWindow>:
    name: "scheduler"
    MDScreen:
        MDTopAppBar:
            id: topappbar
            orientation: "vertical"
            title: "Appointments"
            pos_hint: {"top" : 1}
            elevation: 1
            size_hint: 1, None
            height: "56dp"
            left_action_items:[["arrow-left", lambda x: setattr(app.root, 'current', 'doctor')]]

        MDBoxLayout:
            orientation: 'vertical'
            spacing: "12dp"
            size_hint_y: None
            height: "200dp"
            pos_hint: {"center_x": 0.5, "center_y": 0.7}

            MDTextField:
                id: date_input
                hint_text: "Date"
                size_hint: None, None
                width: root.width*0.8
                on_focus: root.show_date_picker()
                pos_hint: {"center_x": 0.5}
            MDTextField:
                id: time_input
                size_hint: None, None
                width: root.width*0.8
                on_focus: root.show_time_picker()
                hint_text: "Time(HH:MM)"
                pos_hint: {"center_x": 0.5}
            MDTextField:
                id: name_input
                hint_text: "Name"
                size_hint: None, None
                width: root.width*0.8
                pos_hint: {"center_x": 0.5}

        MDRaisedButton:
            text: "Add Appointment"
            size_hint: None, None
            size: 200, 48
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            on_release: root.save_appointment()

        MDBoxLayout:
            orientation: "vertical"
            pos_hint: {"center_x": 0.6, "center_y": 0.42}
            MDLabel:
                text: "Scheduled Appointments"
                size_hint_y: None
                height: self.texture_size[1]
            MDScrollView:  # Scrollable list of appointments
                do_scroll_x: False
                id: appointments_scroll
                size_hint: (None, None)
                size: (root.width * 0.8, root.height * 0.5)
                MDList:
                    height: self.minimum_height
                    id: container


<HospitalWindow>:
    name: "hospital"
    Screen:
        RelativeLayout:
            MDTopAppBar:
                id: topappbar
                orientation: "vertical"
                title: "Hospital Nearby Me"
                pos_hint: {"top" : 1}
                elevation: 1
                size_hint: 1, None
                height: "56dp"  
                left_action_items:[["arrow-left", lambda x: setattr(app.root, 'current', 'home')]]
            
            MDScrollView:
                do_scroll_x: False
                pos_hint: {"top": 0.9, "bottom": 0.1}
                
                MDList:
                    
                    id: hospitalList
                    spacing: dp(10)
                    size_hint: 0.75, None
                    height: self.minimum_height

            BoxLayout:
                size_hint_y: None  
                height: "56dp"     
                pos_hint: {"bottom": 1}  
                elevation : 20
                spacing: 10
                padding: 10
                

                MDTextField:
                    mode: "fill"
                    fill_color: 0,0,0,1
                    id: search_hospital
                    hint_text: "Search by Name"
                    size_hint:None, None
                    width: "240dp"
                    on_text: root.searchHospital(0) if self.hint_text == "Search by Name" else root.searchHospital(1)
                
                MDIconButton:
                    icon: "card-search"
                    text: "Sort"
                    id: sort
                    text : "sort by name"
                    size_hint_x: None
                    on_press: root.toggleSearch()

                MDIconButton:
                    icon: "sort"
                    text: "Sort"
                    id: sort
                    text : "sort by name"
                    size_hint_x: None
                    on_press: root.show_sort_popup()



