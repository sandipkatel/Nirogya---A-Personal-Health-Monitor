<BMIWindow>
    name: "bmi"
    Screen:
        orientation: "vertical"
        MDFloatLayout:
            orientation:"vertical"
            MDTopAppBar:
                id: topappbar
                orientation: "vertical"
                title: "BMI"
                pos_hint: {"top" : 1}
                elevation: 1
                size_hint: 1, None
                height: "56dp"  
                left_action_items:[["arrow-left", lambda x: setattr(app.root, 'current', 'home')]]
            MDLabel:
                pos_hint: {"top" : 0.8}
                halign:"center"
                id:bmi_label
                text:""
                size_hint_y: None
                height: "40dp"
            MDBoxLayout:
                id: tablebox
                orientation: 'vertical'
                spacing: "2dp"
                size_hint:(1,1)
                pos_hint: {"center_x": 0.6, "center_y": 0.9}
                MDBoxLayout:
                    orientation: "horizontal"
                    size_hint_x: None
                    spacing: "2dp"
                    MDTextField:
                        id: bmi_height
                        hint_text: "Height"
                        size_hint: None, None
                        width: "240dp"
                        pos_hint: {"center_x": 0.5}
                    MDRectangleFlatButton:
                        id: drop_down_menu
                        text: 'Unit'
                        size_hint_y: None
                        height: root.height*0.05
                        pos_hint: {'center_x': .5, 'center_y': .1}
                        on_release: root.menu.open()

                MDTextField:
                    id: bmi_weight
                    size_hint: None, None
                    width: root.width*0.8
                    width: "240dp"
                    hint_text: "Weight"
                    pos_hint: {"center_x": 0.33}
                    
                MDRectangleFlatButton:
                    text: "Calculate"
                    background_color: 1, 0, 0, 1  # Red button
                    on_release: root.calculate()
                    size_hint: None, None
                    size: root.width * 0.2, root.height * 0.05
                    pos_hint: {"center_x": 0.33}
            MDBoxLayout:
                id: layout
        
<PredictorWindow>:
    name: "predictor"
    Screen:
        BackgroundLayout:
            id: PredictorScreen
            MDTopAppBar:
                id: topappbar
                orientation: "vertical"
                title: "Disease Predictor"
                pos_hint: {"top" : 1}
                elevation: 1
                size_hint: 1, None
                height: "56dp"  
                left_action_items:[["arrow-left", lambda x: setattr(app.root, 'current', 'home')]]
            
            MDBoxLayout:
                orientation: "vertical"
                pos_hint: {"center_x": 0.5, "y": 0.5}
                MDLabel:
                    text: "Added Symptom:"
                    pos_hint: {"center_x": 0.5, "y": 0.5}
                MDList:
                    id: symptom_list
                    size_hint: (1, None)
                    size: (root.width * 0.5, root.height * 0.4)
            BoxLayout:
                spacing: "12dp"  
                padding: "12dp"     
                MDTextField:
                    id: diseaseTextField
                    hint_text: "Add Symptom"
                    pos_hint: {"center_x": 0.75, "center_y":0.8}
                    size_hint: None, None
                    width: "240dp"
                    on_text : root.show_add_symptom_dropdown()
                    
                MDRectangleFlatButton:
                    text: "Predict"
                    background_color: 1, 0, 0, 1  # Red button
                    on_press: root.predict_disease()
                    size_hint: None, None
                    size: root.width * 0.2, root.height * 0.05
                    pos_hint: {"center_x": 0.5, "center_y": 0.8}
            MDBoxLayout:
                orientation: "vertical"
                pos_hint: {"center_x": 0.6, "y": 0.05}
                ScrollView:
                    id: disease_scroll
                    size: (root.width * 0.2, root.height * 0.3)
                    size_hint: (1, None)
                    MDLabel:
                        id: disease_label
                        text: ""
                        size_hint_y: None
                        height: self.texture_size[1]
<SymptomsWindow>:
    name: "symptoms"

    BackgroundLayout:
        MDTopAppBar:
            id: topappbar
            orientation: "vertical"
            title: "Disease Predictor"
            pos_hint: {"top" : 1}
            elevation: 1
            size_hint: 1, None
            height: "56dp"  
            left_action_items:[["arrow-left", lambda x: setattr(app.root, 'current', 'home')]]
        
        MDTextField:
            id: disease
            hint_text: "Enter a Disease"
            icon_right: "magnify"
            icon_right_color: app.theme_cls.primary_color
            on_text_validate: root.find_info(disease.text)
            size_hint: None, None
            size: 200, 48 
            pos_hint: {"center_x": 0.5, "center_y": 0.8}

        MDBoxLayout:
            orientation: "vertical"
            pos_hint: {"x": 0.3, "center_y": 0.67}
            ScrollView:
                id: symptoms_scroll
                size_hint: (None, None)
                size: (root.width * 0.5, root.height * 0.5)
                MDLabel:
                    id: all_info
                    text: ""
                    size_hint_y: None
                    height: self.texture_size[1]
        
<DoctorWindow>
    name: "doctor"
    MDScreen:
        orientation:"vertical"
