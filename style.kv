
<BackgroundLayout>:
    MDLabel:
        id: background
        size_hint: 10, 10
        canvas.before:
            Color:
                rgba: 1,1,1, 1  # Light gray background
            Rectangle:
                pos: self.pos
                size: self.size

<HomeWindow>:
    name: "home"
    Screen:
        BackgroundLayout:
            orientation: "vertical"
            FitImage:
                source:'./dependencies/pics/background.jpg'
                size: root.width, root.height 
                pos: self.pos

            AsyncImage:
                source: './dependencies/pics/Transparent_Logo.png'
                size_hint: (None, None)
                pos_hint: {"center_x": 0.5, "center_y": 0.9}
                size: root.width * 0.25, root.height * 0.25

                    
            MDGridLayout:
                cols: 3
                spacing: root.width * 0.03
                padding: root.width * 0.1
                size_hint: None, None
